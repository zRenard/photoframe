{
  "name": "photoframe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node server.js",
    "server": "node server.js",
    "dev:server": "nodemon --watch server.js --exec node server.js",
    "dev:full": "concurrently -n 'SERVER,VITE' -c 'bgBlue.bold,bgGreen.bold' \"npm run dev:server\" \"vite\"",
    "build:prod": "npm run build && NODE_ENV=production node server.js",
    "test:vitest": "vitest run --reporter verbose",
    "test:watch": "vitest --reporter verbose",
    "test": "jest --verbose --config=jest.config.cjs",
    "test:coverage": "jest --coverage --config=jest.config.cjs",
    "test:calendar": "jest src/components/__tests__/CalendarPopin.jest.simple.test.jsx --verbose --no-cache --config=jest.config.cjs",
    "test:basic": "jest src/test/basic.test.js --verbose --no-cache --config=jest.config.cjs",
    "test:translations": "jest src/components/__tests__/calendarTranslations.test.js --verbose --no-cache --config=jest.config.cjs",
    "test:superbasic": "jest src/test/super-basic.test.js --verbose --no-cache --config=jest.config.cjs",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --no-cache --config=jest.config.cjs",
    "test:fixed": "jest src/components/__tests__/CalendarPopin.test.jsx src/components/__tests__/CalendarPopin.simplified.test.jsx src/test/basic.test.jsx --verbose --config=jest.config.cjs"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "axios": "^1.10.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.19.2",
    "nodemon": "^3.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-swipeable": "^7.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.25.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "tailwindcss": "^3.4.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
